print(True and True)  # True   i.e. Washington D.C. is the capital of USA and Seoul is the capital of South Korea  #True
print(True and False) # False  i.e. Washington D.C. is the capital of USA and Toronto is the capital of South Korea  #False
print(False and True) # False  i.e. New York is the capital of USA and Seoul is the capital of South Korea  #False
print(False and False) # False i.e. New York is the capital of USA and Toronto is the capital of South Korea  #False

print(True or True)  # True    i.e. Washington D.C. is the capital of USA or Seoul is the capital of South Korea  #True
print(True or False) # True    i.e. Washington D.C. is the capital of USA or Toronto is the capital of South Korea  #True
print(False or True) # True    i.e. New York is the capital of USA or Seoul is the capital of South Korea  #True
print(False or False) # False  i.e. New York is the capital of USA or Toronto is the capital of South Korea  #False
 
print(not True)  # False  i.e. Washington D.C. is not the capital of USA  #False
print(not False) # True   i.e. New York is not the capital of USA  #True


##
print(2>1)   #True
print(2<1)   #False
print(3>=2)  #True
print(3<=3)  #True
print(2==2)  #True
print(2!=2)  #False

print("hello" == "hello")  #True
print("hello" != "hello")  #True


## 
print(not not True)  #True
print(not not false) #False

print(3>1 and "hello" == "hello")  #True and True = True
print(5==5 or (5<3 and 10>5))  #True or False = True

x=5
print(x>7 or not(x<2 or x==5)) #False or False = False
